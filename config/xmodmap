clear control
clear shift
clear lock
clear mod4
clear mod1

keycode  23 = Escape NoSymbol Escape
keycode  62 = Tab ISO_Left_Tab Tab ISO_Left_Tab
keycode  66 = Control_L NoSymbol Control_L
keycode  50 = Shift_L NoSymbol Shift_L
keycode 133 = Super_L NoSymbol Super_L
keycode 134 = Alt_R Meta_R Alt_R Meta_R
keycode 108 = Alt_L Meta_L Alt_L Meta_L
keycode  64 = Super_R NoSymbol Super_R
!keycode  51 = BackSpace BackSpace BackSpace BackSpace
!keycode  22 = backslash bar backslash bar

add control = Control_L Control_R
add mod1 = Alt_L Alt_R
add mod4 = Super_L Super_R Super_R Hyper_L
add shift = Shift_L Shift_R
